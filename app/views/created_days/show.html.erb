<p id="notice"><%= notice %></p>
<h1><%= @created_day.day %>に投稿された日記一覧</h1>

<table class="table">
  <tbody>
  <% @created_day.diaries.each do |diary| %>
    <tr>
      <td><%= diary.title %></td>
      <td><button type="button" class="btn btn-dark"><%= link_to '本文を読む', diary, {class: "text-white"} %></button></td>
    </tr>
  <% end %>
  </tbody>
</table>


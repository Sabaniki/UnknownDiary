<p id="notice"><%= notice %></p>

<h1>日付け一覧</h1>

<table class="table">
  <tbody>
    <% @created_days.each do |created_day| %>
      <tr>
        <td><%= created_day.day %></td>
        <td><button type="button" class="btn btn-dark"><%= link_to 'この日に投稿された日記を見る', created_day, {class: "text-white"} %></button></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

